<%= form_with url: "/banks/#{@bank.id}/branches", method: :post, local: true do |form| %>

<%= form.label :name %>
<%= form.text_field :name %>
<br>
<%= form.label :zip_code %>
<%= form.text_field :zip_code %>
<br>
Has ATM?
<%= form.label :has_atm_true, "True" %>
<%= form.radio_button :has_atm, true %>
<%= form.label :has_atm, "False" %>
<%= form.radio_button :has_atm_false, false %>
<br>
<%= form.label :quarterly_rev %>
<%= form.text_field :quarterly_rev %>
<br>
<%= form.submit "Create Branch" %>
<% end %>
